{
  "name": "mtrics-express",
  "version": "1.3.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jorma16/express-metrics.git"
  },
  "keywords": [
    "express",
    "influx",
    "wrapper",
    "metrics",
    "grafana"
  ],
  "author": "j0rma",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jorma16/express-metrics/issues"
  },
  "homepage": "https://github.com/jorma16/express-metrics#readme",
  "devDependencies": {
    "express": "4.16.2",
    "xo": "0.23.0"
  },
  "dependencies": {
    "geoip-lite": "1.3.5",
    "influxdb-nodejs": "2.11.0",
    "lodash": "4.17.11",
    "on-headers": "1.0.2"
  },
  "xo": {
    "space": true,
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "ignoreRestSiblings": true,
          "args": "none"
        }
      ],
      "no-warning-comments": [
        0
      ],
      "arrow-parens": [
        "error",
        "always"
      ],
      "object-shorthand": [
        "off"
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ]
    },
    "settings": {
      "import/resolver": {
        "node": {
          "paths": [
            "./"
          ]
        }
      }
    }
  }
}
